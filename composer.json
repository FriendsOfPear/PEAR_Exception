{
    "name": "friendsofpear/pear_exception",
    "description": "The PEAR Exception base class.",
    "type": "class",
    "keywords": [
        "exception"
    ],
    "homepage": "https://github.com/FriendsOfPear/PEAR_Exception",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Helgi Thormar",
            "email": "dufuz@php.net"
        },
        {
            "name": "Greg Beaver",
            "email": "cellog@php.net"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "PEAR\\Exception\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PEAR\\Exception\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "include-path": [
        "."
    ],
    "support": {
        "issues": "https://github.com/FriendsOfPear/PEAR_Exception/issues",
        "source": "https://github.com/FriendsOfPear/PEAR_Exception"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.7"
    }
}
